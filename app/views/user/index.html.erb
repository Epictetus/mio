<div class="users">
  <h3>User List</h3>
  <ul class="userLists">
    <% @users.each do |user| %>
      <li>
      <%= user_icon(user) %>
      <p class="name">screen_name: <%= user.screen_name %></p>
      <p class="email">email: <%= user.email %></p>
      <hr class="defloat" />
      </li>
    <% end %>
  </ul>
</div>
